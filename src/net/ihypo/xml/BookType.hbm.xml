<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.ihypo.models.BookType" table="book_types">
        <cache usage="read-write"/>
        <id name="typeId" column="type_id">
            <generator class="native"/>
        </id>
        <property name="typeCode" type="java.lang.String" column="type_code"/>
        <property name="typeName" type="java.lang.String" column="type_name"/>
    </class>
</hibernate-mapping>